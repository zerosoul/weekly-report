{
  "extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
  "rules": {
    "semi": 2,
    "no-console": "off",
    "react/prop-types": 0,
    "react/display-name": "off",
    "no-unused-vars": [
      "warn",
      {
        "ignoreRestSiblings": true
      }
    ],
    "no-sparse-arrays": "error",
    "comma-dangle": ["error", "never"],
    "no-trailing-spaces": "error",
    "no-unreachable": "error",
    "radix": 2
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "globals": {
    "UE": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "serviceworker": true
  },
  "settings": {
    "react": {
      "version": "16.8"
    }
  }
}
